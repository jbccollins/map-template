{
  "name": "webpack-react-esri",
  "version": "1.1.0",
  "description": "React + Redux framework for ArcGIS JavaScript applications with Webpack 4 bundling",
  "main": "index.js",
  "author": "Blue Raster",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch webpack.config.js --watch webpackconfig/ --exec \"webpack-dev-server --open --env development\"",
    "build": "rm -rf ./build; webpack --env production;",
    "deploy": "node ./build-deploy-script.js;",
    "debug": "node --inspect --debug-brk ./node_modules/webpack/bin/webpack.js",
    "lint:js": "eslint src/ webpack.*.js --cache",
    "fix:lint:js": "yarn run lint:js -- --fix",
    "test:jest:watch": "jest --watch",
    "test": "jest --no-cache",
    "test:update": "jest --updateSnapshot",
    "clean:build": "rm -f build/*"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setupTests.js",
    "verbose": true,
    "globals": {
      "NODE_ENV": "test"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/tests/__mocks__/object.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "mini-css-extract-plugin": "^0.8.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.0.1",
    "html-loader": "^0.5.1",
    "html-webpack-externals-plugin": "^3.6.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "jest-cli": "^24.7.1",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.9",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.12.10",
    "redux-devtools": "^3.5.0",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^2.1.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.1.5"
  },
  "prettier": {
    "singleQuote": true
  }
}
